.TH carl_url_dup 3 "6 Aug 2018" "libcarl" "libcarl Manual"
.SH NAME
carl_url_dup - duplicate a CARLU handle
.SH SYNOPSIS
.B #include <carl/carl.h>

CARLU *carl_url_dup(CARLU *inhandle);
.fi
.SH DESCRIPTION
Duplicates a given CARLU \fIinhandle\fP and all its contents and returns a
pointer to a new CARLU handle. The new handle also needs to be freed with
\fIcarl_url_cleanup(3)\fP.
.SH RETURN VALUE
Returns a new handle or NULL if out of memory.
.SH EXAMPLE
.nf
  CARLUcode rc;
  CARLU *url = carl_url();
  CARLU *url2;
  rc = carl_url_set(url, CARLUPART_URL, "https://example.com", 0);
  if(!rc) {
    url2 = carl_url_dup(url); /* clone it! */
    carl_url_cleanup(url2);
  }
  carl_url_cleanup(url);
.fi
.SH AVAILABILITY
Added in carl 7.62.0
.SH "SEE ALSO"
.BR carl_url_cleanup "(3), " carl_url "(3), " carl_url_set "(3), "
.BR carl_url_get "(3), "
